    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns="http://usefulinc.com/ns/doap#">
    <name>jnrbase</name>
    <shortname>jnrbase</shortname>

    <shortdesc xml:lang="en">
        Common utility functionality
    </shortdesc>
    <description xml:lang="en">
        jnrbase is a collection of common utility libraries that are used in
        various Open Source projects that I (JNRowe) work on.  Feel free to use
        it, and especially improve it, in your projects!
    </description>

    <homepage rdf:resource="https://github.com/JNRowe/jnrbase" />
