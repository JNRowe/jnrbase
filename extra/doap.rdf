    <repository>
        <GitRepository>
            <location rdf:resource="git://github.com/JNRowe/jnrbase.git" />
            <browse rdf:resource="https://github.com/JNRowe/jnrbase" />
        </GitRepository>
    </repository>
    <created>2014-01-28</created>
    <license rdf:resource="http://spdx.org/licenses/GPL-3.0+" />
    <programming-language>Python</programming-language>

    <maintainer>
        <foaf:Person>
            <foaf:name>James Rowe</foaf:name>
            <mbox rdf:resource="mailto:jnrowe@gmail.com" />
            <rdfs:homepage rdf:resource="http://jnrowe.github.io" />
        </foaf:Person>
    </maintainer>
</Project>
